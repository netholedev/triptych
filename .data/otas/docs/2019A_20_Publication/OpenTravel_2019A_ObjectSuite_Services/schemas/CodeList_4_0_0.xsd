    <xsd:simpleType name="OrderState_Enum">
        <xsd:annotation>
            <xsd:documentation source="Description">The traveler was not present to receive the product at the specified time. Note, this does not cancel or complete the order.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="EnumerationClosed">OrderState_Enum</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Order-Pending">
                <xsd:annotation>
                    <xsd:documentation source="Description">The supplier has received an order and plans to accept it but is unable to confirm the order at this time.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Confirmed">
                <xsd:annotation>
                    <xsd:documentation source="Description">The supplier has received an order and confirms the plan to fulfill the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Confirmed-Modified">
                <xsd:annotation>
                    <xsd:documentation source="Description">The supplier has received and accepted a request to modify the order in a manner that does not impact the fulfillment of the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Rejected">
                <xsd:annotation>
                    <xsd:documentation source="Description">The supplier has received and decided not to accept an order for some reason.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Waitlisted">
                <xsd:annotation>
                    <xsd:documentation source="Description">The supplier has record of traveler request that may be reviewed and accepted if the requirements for fulfillment are met at later time.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="No-Show">
                <xsd:annotation>
                    <xsd:documentation source="Description">The traveler was not present to receive the product at the specified time. Note, this does not cancel or complete the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Completed">
                <xsd:annotation>
                    <xsd:documentation source="Description">The supplier provided the product(s) and/or services to the traveler.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Cancellation-Pending">
                <xsd:annotation>
                    <xsd:documentation source="Description">The cancellation has been requested and is in process.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Cancelled">
                <xsd:annotation>
                    <xsd:documentation source="Description">The order has been cancelled at the request of either the supplier or customer.  The supplier has released hold on resources required to fulfill the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    
